assume n >= 0 ;
assume p0 = p; havoc p; assume p = 0;
assume x0 = x; havoc x; assume x = 0;
assert p = x * m && x <= n;
havoc x; havoc p; assume p = x * m && x <= n;
((assume x < n;
  assume x1 = x; havoc x; assume x = x1 + 1;
  assume p1 = p; havoc p; assume p = p1 + m;
  assert p = x * m && x <= n; assume false;)
  []
  assume x >= n;)
assert p = n * m;
